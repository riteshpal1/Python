    "from turtle import *\n",
    "\n",
    "start = 300\n",
    "goto(-150,-150)\n",
    "while start > 0:\n",
    "  forward(start)\n",
    "  left(360/6)\n",
    "  start -= 10\n",
    "\n",
    "  write(start, font=('Arial', 8, 'normal'))\n",
    "\n",
    "mainloop()"
